From 7935f7fc3f490890af134e59b672daa29a65d817 Mon Sep 17 00:00:00 2001
From: Ikey Doherty <ikey@solus-project.com>
Date: Sun, 16 Oct 2016 01:34:20 +0100
Subject: [PATCH 5/7] panel: Switch to a top panel by default for
 traditionalismismismisms

Signed-off-by: Ikey Doherty <ikey@solus-project.com>
---
 panel/solus-live.layout | 18 +++++++++---------
 panel/solus.layout      | 18 +++++++++---------
 2 files changed, 18 insertions(+), 18 deletions(-)

diff --git a/panel/solus-live.layout b/panel/solus-live.layout
index be9b5fa..b13a6c8 100644
--- a/panel/solus-live.layout
+++ b/panel/solus-live.layout
@@ -1,39 +1,39 @@
-[Toplevel bottom]
+[Toplevel top]
 expand=true
-orientation=bottom
+orientation=top
 size=24
 
 [Object menu-bar]
 object-type=menu-bar
-toplevel-id=bottom
+toplevel-id=top
 position=0
 locked=true
 
 [Object web-browser]
 object-type=launcher
 launcher-location=/usr/share/applications/firefox.desktop
-toplevel-id=bottom
+toplevel-id=top
 position=10
 locked=true
 
 [Object install]
 object-type=launcher
 launcher-location=/usr/share/applications/os-installer-gtk.desktop
-toplevel-id=bottom
+toplevel-id=top
 position=20
 locked=true
 
 [Object window-list]
 object-type=applet
 applet-iid=WnckletFactory::WindowListApplet
-toplevel-id=bottom
+toplevel-id=top
 position=30
 locked=true
 
 [Object notification-area]
 object-type=applet
 applet-iid=NotificationAreaAppletFactory::NotificationArea
-toplevel-id=bottom
+toplevel-id=top
 position=20
 panel-right-stick=true
 locked=true
@@ -41,7 +41,7 @@ locked=true
 [Object clock]
 object-type=applet
 applet-iid=ClockAppletFactory::ClockApplet
-toplevel-id=bottom
+toplevel-id=top
 position=10
 panel-right-stick=true
 locked=true
@@ -49,7 +49,7 @@ locked=true
 [Object show-desktop]
 object-type=applet
 applet-iid=WnckletFactory::ShowDesktopApplet
-toplevel-id=bottom
+toplevel-id=top
 position=0
 panel-right-stick=true
 locked=true
diff --git a/panel/solus.layout b/panel/solus.layout
index 17f0dfa..da0eb64 100644
--- a/panel/solus.layout
+++ b/panel/solus.layout
@@ -1,39 +1,39 @@
-[Toplevel bottom]
