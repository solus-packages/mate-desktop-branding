From 9718ffcb2685d7b799d0bfe8e8258d5404a7ba26 Mon Sep 17 00:00:00 2001
From: Ikey Doherty <ikey@solus-project.com>
Date: Sun, 16 Oct 2016 01:49:07 +0100
Subject: [PATCH 6/8] panel: Remove thoroughly derpy launcher icons from panels

Signed-off-by: Ikey Doherty <ikey@solus-project.com>
---
 panel/solus-live.layout | 16 +---------------
 panel/solus.layout      | 16 +---------------
 2 files changed, 2 insertions(+), 30 deletions(-)

diff --git a/panel/solus-live.layout b/panel/solus-live.layout
index b13a6c8..6184e62 100644
--- a/panel/solus-live.layout
+++ b/panel/solus-live.layout
@@ -9,25 +9,11 @@ toplevel-id=top
 position=0
 locked=true
 
-[Object web-browser]
-object-type=launcher
-launcher-location=/usr/share/applications/firefox.desktop
-toplevel-id=top
-position=10
-locked=true
-
-[Object install]
-object-type=launcher
-launcher-location=/usr/share/applications/os-installer-gtk.desktop
-toplevel-id=top
-position=20
-locked=true
-
 [Object window-list]
 object-type=applet
 applet-iid=WnckletFactory::WindowListApplet
 toplevel-id=top
-position=30
+position=10
 locked=true
 
 [Object notification-area]
diff --git a/panel/solus.layout b/panel/solus.layout
index da0eb64..6184e62 100644
--- a/panel/solus.layout
+++ b/panel/solus.layout
@@ -9,25 +9,11 @@ toplevel-id=top
 position=0
 locked=true
 
-[Object web-browser]
-object-type=launcher
-launcher-location=/usr/share/applications/firefox.desktop
-toplevel-id=top
-position=10
-locked=true
-
-[Object software-center]
-object-type=launcher
-launcher-location=/usr/share/applications/solus-sc.desktop
-toplevel-id=top
-position=20
-locked=true
-
 [Object window-list]
 object-type=applet
 applet-iid=WnckletFactory::WindowListApplet
 toplevel-id=top
-position=30
+position=10
 locked=true
 
 [Object notification-area]
